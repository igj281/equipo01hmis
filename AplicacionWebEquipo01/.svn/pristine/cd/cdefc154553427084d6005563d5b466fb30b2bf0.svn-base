<center>
<div class="page-header"><h1>Registro de nuevo usuario</h1></div>
</center>
<!-- Le styles -->
    <link href="../css/bootstrap.min.css" rel="stylesheet">
    <style type="text/css">
      body {
        <center>
        padding-top: 40px;
        padding-bottom: 40px;
        background-color: #f5f5f5;
      </center>
      }


      .form-signin {
        max-width: 300px;
        padding: 19px 29px 29px;
        margin: 50 auto 20px;
        background-color: #fff;
        border: 1px solid #e5e5e5;
        -webkit-border-radius: 5px;
           -moz-border-radius: 5px;
                border-radius: 5px;
        -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.05);
           -moz-box-shadow: 0 1px 2px rgba(0,0,0,.05);
                box-shadow: 0 1px 2px rgba(0,0,0,.05);
      }
      .form-signin .form-signin-heading,
      .form-signin .checkbox {
        margin-bottom: 10px;
      }
      .form-signin input[type="text"],
      .form-signin input[type="password"] {
        font-size: 16px;
        height: auto;
        margin-bottom: 15px;
        padding: 7px 9px;
      }

    </style>
    <div class="container">
 <div style="text-align:center;">
    <link href="../assets/css/bootstrap-responsive.css" rel="stylesheet">
    <form name="registro" action="/registroupload" method="post">
        <label>Usuario</label>
        <br/>
        <input type="text" name="usuario" placeholder="Usuario" onkeyup="validarUsuario()">
        

        <br/>
        <label>Contraseña</label>
        <br/>
        <input type="password" name="password" placeholder="Contraseña" onkeyup="validarContraseña()">
        <br/>
        <label>Repetir contraseña</label>
        <br/>
        <input type="password" name="confirmarpassword" placeholder="Contraseña" onkeyup="validarConfirmarContraseña()">
        <br/>
        <label>E-mail</label>
        <br/>
        <input type="text" name="email" placeholder="E-mail" onkeyup="validarEmail()">
        <br/>
        <label>Nombre</label>
        <br/>
        <input type="text" name="nombre" placeholder="Nombre" onkeyup="validarNombre()">
        <br/>
        <label>Apellidos</label>
        <br/>
        <input type="text" name="apellidos" placeholder="Apellidos" onkeyup="validarApellidos()">
        <br/>
        <label>Teléfono</label>
         <br/>
        <input type="text" name="telefono" placeholder="Teléfono" onkeyup="validarTelefono()">
        <br/>    
        <br/>     
        <div class="control-groups">
            <a href ="logueados"<a  onclick="validarFormulario()" class="btn btn-large btn-primary" href="/logueados">Registrar</a>
            <a href ="index"<a class="btn btn-large btn-primary" href="/">Cancelar</a>
        </div>
    </form>
</div> <!-- /container -->
</div>

